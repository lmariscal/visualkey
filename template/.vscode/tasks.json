{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "visualkey",
      "type": "shell",
      "command": "visualkey run ${workspaceFolder}",
      "problemMatcher": ["$msCompile"],
      "presentation": {
        "echo": true,
        "reveal": "always",
        "focus": false,
        "panel": "dedicated",
        "showReuseMessage": false,
        "clear": true
      },
      "group": {
        "kind": "build",
        "isDefault": true
      }
    }
  ]
}
